{
    "extends": "standard",
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "global-strict": ["error", "always"],
        "indent": ["error", 4],
        "semi": ["error", "always"],
        "space-before-function-paren": ["error", "never"],
         "eqeqeq": [false]
    }
}
